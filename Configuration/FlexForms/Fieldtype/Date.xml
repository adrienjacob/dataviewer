<T3DataStructure>
    <ROOT>
        <type>array</type>
        <el>
            <info>
                <TCEforms>
                    <label></label>
                    <config>
                        <type>user</type>
                        <userFunc>MageDeveloper\Dataviewer\UserFunc\Text->displayTemplate</userFunc>
                        <parameters>
                            <template>EXT:dataviewer/Resources/Private/Templates/FieldConfiguration/Notice.html</template>
                        </parameters>
                    </config>
                </TCEforms>
            </info>
            <eval>
                <TCEforms>
                    <label>eval</label>
                    <onChange>reload</onChange>
                    <config>
                        <type>select</type>
                        <multiple>1</multiple>
                        <autosizemax>5</autosizemax>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">required</numIndex>
                                <numIndex index="1">required</numIndex>
                            </numIndex>
                        </items>
                        <maxitems>9999</maxitems>
                        <minitems>0</minitems>
                        <size>5</size>
                    </config>
                </TCEforms>
            </eval>
            <placeholder>
                <TCEforms>
                    <label>placeholder</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </placeholder>
            <range_lower>
                <TCEforms>
                    <label>range -> lower</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </range_lower>
            <range_upper>
                <TCEforms>
                    <label>range -> upper</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </range_upper>
        </el>
    </ROOT>
</T3DataStructure>
