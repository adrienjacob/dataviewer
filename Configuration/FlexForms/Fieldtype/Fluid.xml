<T3DataStructure>
    <ROOT>
        <type>array</type>
        <el>
            <info>
                <TCEforms>
                    <label></label>
                    <config>
                        <type>user</type>
                        <userFunc>MageDeveloper\Dataviewer\UserFunc\Text->displayTemplate</userFunc>
                        <parameters>
                            <template>EXT:dataviewer/Resources/Private/Templates/FieldConfiguration/Fluid.html</template>
                        </parameters>
                    </config>
                </TCEforms>
            </info>
            <showInBackend>
                <TCEforms>
                    <label>Show in Backend</label>
                    <config>
                        <type>check</type>
                        <default>1</default>
                    </config>
                </TCEforms>
            </showInBackend>
            <generateOutput>
                <TCEforms>
                    <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.generate_output</label>
                    <config>
                        <type>check</type>
                        <default>0</default>
                    </config>
                </TCEforms>
            </generateOutput>
            <injectVariables>
                <TCEforms>
                    <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.variable_injection</label>
                    <config>
                        <type>select</type>
                        <renderType>selectMultipleSideBySide</renderType>
                        <multiple>0</multiple>
                        <autosizemax>10</autosizemax>
                        <foreign_table>tx_dataviewer_domain_model_variable</foreign_table>
                        <foreign_table_where>ORDER BY tx_dataviewer_domain_model_variable.pid ASC</foreign_table_where>
                        <maxitems>9999</maxitems>
                        <minitems>0</minitems>
                        <size>5</size>
                    </config>
                </TCEforms>
            </injectVariables>
        </el>
    </ROOT>
</T3DataStructure>
