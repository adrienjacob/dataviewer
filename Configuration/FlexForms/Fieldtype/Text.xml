<T3DataStructure>
    <ROOT>
        <type>array</type>
        <el>
            <info>
                <TCEforms>
                    <label></label>
                    <config>
                        <type>user</type>
                        <userFunc>MageDeveloper\Dataviewer\UserFunc\Text->displayTemplate</userFunc>
                        <parameters>
                            <template>EXT:dataviewer/Resources/Private/Templates/FieldConfiguration/Notice.html</template>
                        </parameters>
                    </config>
                </TCEforms>
            </info>
            <eval>
                <TCEforms>
                    <label>eval</label>
                    <config>
                        <type>select</type>
                        <renderType>selectMultipleSideBySide</renderType>
                        <multiple>0</multiple>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">required</numIndex>
                                <numIndex index="1">required</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">trim</numIndex>
                                <numIndex index="1">trim</numIndex>
                            </numIndex>
                        </items>
                        <maxitems>9999</maxitems>
                        <minitems>0</minitems>
                    </config>
                </TCEforms>
            </eval>
            <renderType>
                <TCEforms>
                    <label>renderType</label>
                    <onChange>reload</onChange>
                    <config>
                        <type>select</type>
                        <renderType>selectSingle</renderType>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">normal</numIndex>
                                <numIndex index="1"></numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">t3editor</numIndex>
                                <numIndex index="1">t3editor</numIndex>
                            </numIndex>
                            <numIndex index="2" type="array">
                                <numIndex index="0">belayoutwizard</numIndex>
                                <numIndex index="1">belayoutwizard</numIndex>
                            </numIndex>
                        </items>
                        <minitems>0</minitems>
                        <maxitems>1</maxitems>
                    </config>
                </TCEforms>
            </renderType>
            <format>
                <TCEforms>
                    <label>format</label>
                    <displayCond>FIELD:renderType:=:t3editor</displayCond>
                    <config>
                        <type>select</type>
                        <renderType>selectSingle</renderType>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">html</numIndex>
                                <numIndex index="1">html</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">typoscript</numIndex>
                                <numIndex index="1">typoscript</numIndex>
                            </numIndex>
                            <numIndex index="2" type="array">
                                <numIndex index="0">javascript</numIndex>
                                <numIndex index="1">javascript</numIndex>
                            </numIndex>
                            <numIndex index="3" type="array">
                                <numIndex index="0">css</numIndex>
                                <numIndex index="1">css</numIndex>
                            </numIndex>
                            <numIndex index="4" type="array">
                                <numIndex index="0">xml</numIndex>
                                <numIndex index="1">xml</numIndex>
                            </numIndex>
                            <numIndex index="5" type="array">
                                <numIndex index="0">php</numIndex>
                                <numIndex index="1">php</numIndex>
                            </numIndex>
                            <numIndex index="6" type="array">
                                <numIndex index="0">sparql</numIndex>
                                <numIndex index="1">sparql</numIndex>
                            </numIndex>
                        </items>
                        <minitems>0</minitems>
                        <maxitems>1</maxitems>
                    </config>
                </TCEforms>
            </format>
            <cols>
                <TCEforms>
                    <label>cols</label>
                    <config>
                        <type>input</type>
                        <size>2</size>
                        <eval>int</eval>
                        <default>30</default>
                    </config>
                </TCEforms>
            </cols>
            <rows>
                <TCEforms>
                    <label>rows</label>
                    <config>
                        <type>input</type>
                        <size>2</size>
                        <eval>int</eval>
                        <default>5</default>
                    </config>
                </TCEforms>
            </rows>
            <placeholder>
                <TCEforms>
                    <label>placeholder</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </placeholder>
            <max>
                <TCEforms>
                    <label>max</label>
                    <config>
                        <type>input</type>
                        <size>5</size>
                        <eval>int</eval>
                        <default>2000</default>
                    </config>
                </TCEforms>
            </max>
            <wrap>
                <TCEforms>
                    <label>wrap</label>
                    <config>
                        <type>select</type>
                        <renderType>selectSingle</renderType>
                        <multiple>0</multiple>
                        <items type="array">
                            <numIndex index="0" type="array">
                                <numIndex index="0">off</numIndex>
                                <numIndex index="1">off</numIndex>
                            </numIndex>
                            <numIndex index="1" type="array">
                                <numIndex index="0">virtual</numIndex>
                                <numIndex index="1">virtual</numIndex>
                            </numIndex>
                        </items>
                        <maxitems>1</maxitems>
                        <minitems>0</minitems>
                    </config>
                </TCEforms>
            </wrap>
        </el>
    </ROOT>
</T3DataStructure>
