<T3DataStructure>
    <ROOT>
        <type>array</type>
        <el>
            <info>
                <TCEforms>
                    <label></label>
                    <config>
                        <type>user</type>
                        <userFunc>MageDeveloper\Dataviewer\UserFunc\Text->displayTemplate</userFunc>
                        <parameters>
                            <template>EXT:dataviewer/Resources/Private/Templates/FieldConfiguration/Notice.html</template>
                        </parameters>
                    </config>
                </TCEforms>
            </info>
            <allowed>
                <TCEforms>
                    <label>allowed</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </allowed>
            <disallowed>
                <TCEforms>
                    <label>disallowed</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                        <default>php,php5</default>
                    </config>
                </TCEforms>
            </disallowed>
            <showUpload>
                <TCEforms>
                    <label>showUpload</label>
                    <config>
                        <checkbox>1</checkbox>
                        <type>check</type>
                    </config>
                </TCEforms>
            </showUpload>
        </el>
    </ROOT>
</T3DataStructure>
