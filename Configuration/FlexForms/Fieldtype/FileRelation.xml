<T3DataStructure>
    <ROOT>
        <type>array</type>
        <el>
            <info>
                <TCEforms>
                    <label></label>
                    <config>
                        <type>user</type>
                        <userFunc>MageDeveloper\Dataviewer\UserFunc\Text->displayTemplate</userFunc>
                        <parameters>
                            <template>EXT:dataviewer/Resources/Private/Templates/FieldConfiguration/Notice.html</template>
                        </parameters>
                    </config>
                </TCEforms>
            </info>
            <allowed>
                <TCEforms>
                    <label>allowed</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                    </config>
                </TCEforms>
            </allowed>
            <disallowed>
                <TCEforms>
                    <label>disallowed</label>
                    <config>
                        <type>input</type>
                        <size>15</size>
                        <default>php,php5</default>
                    </config>
                </TCEforms>
            </disallowed>
            <fileUploadAllowed>
                <TCEforms>
                    <label>fileUploadAllowed</label>
                    <config>
                        <checkbox>1</checkbox>
                        <type>check</type>
                    </config>
                </TCEforms>
            </fileUploadAllowed>
            <minitems>
                <TCEforms>
                    <label>minitems</label>
                    <config>
                        <type>input</type>
                        <size>3</size>
                        <eval>int</eval>
                        <default>0</default>
                    </config>
                </TCEforms>
            </minitems>
            <maxitems>
                <TCEforms>
                    <label>maxitems</label>
                    <config>
                        <type>input</type>
                        <size>3</size>
                        <eval>int</eval>
                        <default>1</default>
                    </config>
                </TCEforms>
            </maxitems>
        </el>
    </ROOT>
</T3DataStructure>
