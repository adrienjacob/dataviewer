<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <general_settings>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.general_settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.logo>
                        <TCEforms>
                            <label></label>
                            <config>
                                <type>user</type>
                                <userFunc>MageDeveloper\Dataviewer\UserFunc\Logo->displayLogoText</userFunc>
                            </config>
                        </TCEforms>
                    </settings.logo>
                    <settings.datatype_selection>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.record_save_datatype</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <multiple>0</multiple>
                                <items type="array"></items>
                                <foreign_table>tx_dataviewer_domain_model_datatype</foreign_table>
                                <foreign_table_where><![CDATA[AND 0 < FIND_IN_SET(tx_dataviewer_domain_model_datatype.pid, (SELECT pages FROM tt_content WHERE uid=###THIS_UID###))]]></foreign_table_where>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <size>1</size>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.datatype_selection>
                    <settings.file_upload_folder>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.file_upload_folder</label>
                            <config type="array">
                                <type>group</type>
                                <internal_type>folder</internal_type>
                                <show_thumbs>0</show_thumbs>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                                <eval></eval>
                            </config>
                        </TCEforms>
                    </settings.file_upload_folder>
                    <settings.allowed_actions>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.allowed_actions</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.action_new</numIndex>
                                        <numIndex index="1">new</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.action_edit</numIndex>
                                        <numIndex index="1">edit</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.action_delete</numIndex>
                                        <numIndex index="1">delete</numIndex>
                                    </numIndex>
                                </items>
                                <size>4</size>
                                <multiple>0</multiple>
                                <minitems>0</minitems>
                                <maxitems>9999</maxitems>
                            </config>
                        </TCEforms>
                    </settings.allowed_actions>
                </el>
            </ROOT>
        </general_settings>
        <redirect_setting>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.redirect_settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.redirect_success_new>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.redirect_success_new</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.redirect_success_new>
                    <settings.redirect_success_edit>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.redirect_success_edit</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.redirect_success_edit>
                    <settings.redirect_success_delete>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.redirect_success_delete</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.redirect_success_delete>
                </el>
            </ROOT>
        </redirect_setting>
        <template_settings>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.template_settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.template_override>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.template_override</label>
                            <config type="array">
                                <type>group</type>
                                <internal_type>file</internal_type>
                                <allowed>html</allowed>
                                <max_size>2000</max_size>
                                <show_thumbs>0</show_thumbs>
                                <size>1</size>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <eval></eval>
                            </config>
                        </TCEforms>
                    </settings.template_override>
                    <settings.variable_injection>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.variable_injection</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <multiple>0</multiple>
                                <autosizemax>10</autosizemax>
                                <items type="array"></items>
                                <foreign_table>tx_dataviewer_domain_model_variable</foreign_table>
                                <foreign_table_where><![CDATA[AND 0 < FIND_IN_SET(tx_dataviewer_domain_model_variable.pid, (SELECT pages FROM tt_content WHERE uid=###THIS_UID###))]]></foreign_table_where>
                                <maxitems>99999</maxitems>
                                <minitems>0</minitems>
                                <size>5</size>
                            </config>
                        </TCEforms>
                    </settings.variable_injection>
                    <settings.inline_variable_injection>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.inline_variable_injection</label>
                            <config>
                                <type>inline</type>
                                <autosizemax>10</autosizemax>
                                <items type="array"></items>
                                <foreign_table>tx_dataviewer_domain_model_variable</foreign_table>
                                <maxitems>99999</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.inline_variable_injection>
                </el>
            </ROOT>
        </template_settings>
        <developer_settings>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.developer_settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.debug>
                        <TCEforms>
                            <label>LLL:EXT:dataviewer/Resources/Private/Language/locallang.xlf:flexform.debug</label>
                            <config>
                                <checkbox>1</checkbox>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.debug>
                </el>
            </ROOT>
        </developer_settings>
    </sheets>
</T3DataStructure> 
